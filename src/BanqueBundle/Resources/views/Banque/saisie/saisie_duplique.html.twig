                    {% endfor %}
                </select>
            </div>
        </div>

        <div class="form-group"><label class="col-lg-4 control-label">Sous catégorie</label>
            <div class="col-lg-8">
                <select class="form-control" id="dsoussouscategorie">
                    <option value="-1"></option>
                </select>
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-2 pull-right">
                <span class="btn btn-sm btn-default" id="js_form_duplique_submit">Valider</span>
            </div>
        </div>
    </div>
</div>