<div class="row">
    <div class="form-horizontal">
        <div class="form-group"><label class="col-lg-4 control-label">Categorie</label>
            <div class="col-lg-8">
                <select class="form-control" disabled>
                    <option value="16">Banque</option>
                </select>
            </div>
        </div>

        <div class="form-group"><label class="col-lg-4 control-label">Sous catégorie</label>
            <div class="col-lg-8">
                <select class="form-control" id="dsouscategorie">
                    {% for souscategorie in souscategories %}
